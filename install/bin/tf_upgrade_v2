#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'tensorflow-gpu==2.0.0+nv19.12.tf2','console_scripts','tf_upgrade_v2'
__requires__ = 'tensorflow-gpu==2.0.0+nv19.12.tf2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('tensorflow-gpu==2.0.0+nv19.12.tf2', 'console_scripts', 'tf_upgrade_v2')()
    )
